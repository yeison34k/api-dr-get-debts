{
    "compilerOptions": {
      "target": "es6",
      "module": "commonjs",
      "preserveConstEnums": true,
      "strictNullChecks": true,
      "sourceMap": true,
      "allowJs": true,
      "outDir": ".build",
      "moduleResolution": "node",
      "lib": ["es2015"],
      "rootDir": "./"
    },
    "exclude": [
      "node_modules"
    ],
    "types": [
      "node",
      "aws-sdk"
    ]
  }